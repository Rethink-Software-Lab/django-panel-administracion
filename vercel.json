{
  "builds": [
    {
      "src": "project_inventario/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "project_inventario/wsgi.py"
    },
    {
      "src": "/staticfiles/(.*)",
      "dest": "/staticfiles/$1"
    }
  ]
}
